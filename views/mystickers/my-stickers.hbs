<div class="new-sticker container mb-3 mt-3 d-flex">
  <a href="/my-stickers/create" id="addNewSticker" type="button" class="bi bi-plus-square"></a>
</div>

<div class="container grid text-center">
  {{#if stickers.length}}
  {{#each stickers}}
  <div data-stickerid="{{id}}" class="card text-bg-warning mb-3 sticker-card">

    <h5 class="card-header">{{date}}</h5>
    <div class="card-body bg-light">
      <p class="card-title">{{header}}</p>
      {{#each data}}
      <p class="text-start m-1">{{@key}}</p>
      {{!-- <ul class="list-unstyled text-start">
        {{#each note }}
        <li>Note - {{@this}}</li>
        {{/each}}
      </ul> --}}
      {{/each}}
    </div>
    <span {{!-- data-stickerid="{{id}}" --}} type="button"
      class='sticker-trash btn btn-outline-secondary border-0 bi bi-trash'></span>
  </div>
  {{/each}}
  {{else}}
  <p>Stickers is empty</p>
  {{/if}}
</div>
